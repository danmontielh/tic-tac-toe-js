{
  "parserOptions": { "ecmaVersion": 6 },
  "extends": "airbnb-base",
    rules: {
    'no-shadow': 0,
    'no-param-reassign': 0,
    'eol-last': 0,
    'no-unused-vars': 0,
    'no-undef': 0,
    'no-trailing-spaces': 0,
    'no-unused-expressions': 0,
    'arrow-body-style': ["error", "never"]
  }
}